<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_background"
    android:orientation="vertical"
    tools:context="com.xygame.sg.activity.personal.EditSecondCategoryActivity">

    <include layout="@layout/sg_head_layout" />

    <include layout="@layout/gray_divider_line" />
    <include layout="@layout/gray_divider_wide_line" />

    <include layout="@layout/gray_divider_line" />

    <com.xygame.sg.widget.ClearEditText
        android:id="@+id/category_et"
        style="@style/MEditText"
        android:hint="请选取或输入报价单位" />

    <include layout="@layout/gray_divider_line" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:text="选取报价单位" />

    <include layout="@layout/gray_divider_line" />

    <ListView
        android:id="@+id/category_lv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:cacheColorHint="#00000000"
        android:divider="@color/transparent"
        android:dividerHeight="0dp"
        android:scrollbars="none" />




</LinearLayout>
